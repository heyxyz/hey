{
  "name": "@hey/api",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "pnpm codegen; next build",
    "codegen": "prisma generate --schema ./db/schema.prisma",
    "dev": "next dev --port 4785",
    "postinstall": "pnpm codegen",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.ts,.tsx",
    "prettier": "prettier --check \"**/*.{js,ts,tsx,md}\"  --cache",
    "prettier:fix": "prettier --write \"**/*.{js,ts,tsx,md}\"  --cache",
    "prisma:format": "prisma format --schema ./db/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ./db/schema.prisma",
    "prisma:studio": "prisma studio --schema ./db/schema.prisma",
    "start": "next start --port 4785",
    "test:dev": "vitest run",
    "typecheck": "tsc --pretty"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.454.0",
    "@clickhouse/client-web": "^0.2.6",
    "@hey/data": "workspace:*",
    "@hey/lib": "workspace:*",
    "@irys/sdk": "^0.1.0",
    "@prisma/client": "^5.6.0",
    "axios": "^1.6.2",
    "fast-xml-parser": "^4.3.2",
    "ioredis": "^5.3.2",
    "linkedom": "^0.16.4",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "request-ip": "^3.3.0",
    "ua-parser-js": "^1.0.37",
    "urlcat": "^3.1.0",
    "viem": "^1.19.9",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hey/config": "workspace:*",
    "@hey/types": "workspace:*",
    "@types/react": "^18.2.38",
    "@types/request-ip": "^0.0.41",
    "@types/ua-parser-js": "^0.7.39",
    "prisma": "^5.6.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.5"
  }
}
