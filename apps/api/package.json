{
  "name": "@hey/api",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "vite build",
    "codegen": "prisma generate --schema ./src/db/schema.prisma",
    "dev": "nodemon -w src -x tsx src/server.ts",
    "postinstall": "pnpm codegen",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.ts,.tsx",
    "prettier": "prettier --check \"**/*.{js,ts,tsx,md}\"  --cache",
    "prettier:fix": "prettier --write \"**/*.{js,ts,tsx,md}\"  --cache",
    "prisma:format": "prisma format --schema ./src/db/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ./src/db/schema.prisma",
    "prisma:studio": "prisma studio --schema ./src/db/schema.prisma",
    "start": "NODE_ENV=production tsx src/server.ts",
    "test:dev": "vitest run",
    "typecheck": "tsc --pretty"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.485.0",
    "@clickhouse/client": "^0.2.7",
    "@hey/abis": "workspace:*",
    "@hey/data": "workspace:*",
    "@hey/lib": "workspace:*",
    "@irys/sdk": "^0.1.1",
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "country-code-lookup": "^0.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-file-routing": "^3.0.3",
    "fast-xml-parser": "^4.3.2",
    "linkedom": "^0.16.6",
    "node-zendesk": "^5.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "request-ip": "^3.3.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "ua-parser-js": "^1.0.37",
    "urlcat": "^3.1.0",
    "uuid": "^9.0.1",
    "viem": "^2.0.0",
    "vite-express": "^0.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hey/config": "workspace:*",
    "@hey/types": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.15",
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.47",
    "@types/request-ip": "^0.0.41",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.3"
  }
}
