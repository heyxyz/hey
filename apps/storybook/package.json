    "@storybook/theming": "^7.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.11",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.6",
    "autoprefixer": "^10.4.14",
    "eslint-config-weblint": "workspace:*",
