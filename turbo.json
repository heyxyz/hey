{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": { "outputs": [] },
    "lint:fix": { "outputs": [] },
    "prettier": { "outputs": [] },
    "prettier:fix": { "outputs": [] },
    "typecheck": { "outputs": [] },
    "codegen": { "outputs": [] },
    "extract": { "outputs": [] },
    "worker:deploy": { "outputs": [], "cache": false },
    "dev": { "cache": false },
    "start": { "cache": false },
    "test:dev": { "cache": false },
    "test:nightly": { "cache": false }
  }
}
